										<headerToolbar>
											<Toolbar>
												<Button icon="sap-icon://add" press=".onNew.call($controller,$event, $controller.getView().getBindingContext().getPath() + '/MaleName', '/NameProbability')"/>
												<Button icon="sap-icon://delete" text="Delete all" press=".onDeleteAll.call($controller,$event, $controller.getView().getBindingContext().getPath() + '/MaleName')"/>
											</Toolbar>
										</headerToolbar>
										<columns>
											<Column>
