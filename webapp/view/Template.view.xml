												<cells>
													<Input value="{Probability}"/>
												</cells>
												<cells>
													<Button icon="sap-icon://delete" press=".onDelete.call($controller,$event, $controller.getView().getBindingContext().getPath() + '/Gender')"/>
												</cells>
											</ColumnListItem>
										</items>
									</Table>
								</ScrollContainer>
							</l:BlockLayoutCell>
						</l:BlockLayoutRow>
					</l:BlockLayout>
				</IconTabFilter>
				<IconTabFilter text="Deities">
					<l:BlockLayout background="Dashboard">
						<l:BlockLayoutRow>
							<l:BlockLayoutCell width="1">
								<ScrollContainer>
									<Table items="{ path: 'Deity'}" growing="true">
										<headerToolbar>
											<Toolbar>
												<Button icon="sap-icon://add" press=".onNew.call($controller,$event, $controller.getView().getBindingContext().getPath() + '/Deity', '/NameProbabilityDescription')"/>
												<Button icon="sap-icon://delete" text="Delete all" press=".onDeleteAll.call($controller,$event, $controller.getView().getBindingContext().getPath() + '/Deity')"/>
