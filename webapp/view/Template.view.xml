<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" controllerName="NPCGen.NPCGen.controller.Template">
	<Page showHeader="false">
		<IconTabBar expandable="false" class="sapUiResponsiveContentPadding">
			<items>
				<IconTabFilter text="Female Names">
					<l:BlockLayout background="Dashboard">
						<l:BlockLayoutRow>
							<l:BlockLayoutCell width="1">
								<ScrollContainer>
									<Table items="{ path: 'FemaleName'}" growing="true">
										<headerToolbar>
											<Toolbar>
												<Button icon="sap-icon://add" press=".onNew.call($controller,$event, $controller.getView().getBindingContext().getPath() + '/FemaleName', '/NameProbability')"/>
												<Button icon="sap-icon://delete" text="Delete all" press=".onDeleteAll.call($controller,$event, $controller.getView().getBindingContext().getPath() + '/FemaleName')"/>
											</Toolbar>
										</headerToolbar>
										<columns>
											<Column>
												<Text text="Name"/>
											</Column>
											<Column>
												<Text text="Probabilty"/>
											</Column>
											<Column/>
										</columns>
										<items>
											<ColumnListItem>
												<cells>
													<Input value="{Name}"/>
												</cells>
												<cells>
													<Input value="{Probability}"/>
												</cells>
												<cells>
													<Button icon="sap-icon://delete" press=".onDelete.call($controller,$event, $controller.getView().getBindingContext().getPath() + '/FemaleName')"/>
												</cells>
											</ColumnListItem>
										</items>
									</Table>
								</ScrollContainer>
							</l:BlockLayoutCell>
						</l:BlockLayoutRow>
					</l:BlockLayout>
				</IconTabFilter>
				<IconTabFilter text="Male Names">
					<l:BlockLayout background="Dashboard">
						<l:BlockLayoutRow>
							<l:BlockLayoutCell width="1">
								<ScrollContainer>
