				</IconTabFilter>
				<IconTabFilter text="Class">
					<l:BlockLayout background="Dashboard">
						<l:BlockLayoutRow>
							<l:BlockLayoutCell width="1">
								<ScrollContainer>
									<Table items="{ path: 'Class'}" growing="true" itemPress=".onNavToItemClass.call($controller, $event, 'Class')">
										<headerToolbar>
											<Toolbar>
												<Button icon="sap-icon://add" press=".onNew.call($controller,$event, $controller.getView().getBindingContext().getPath() + '/Class/', '/ClassProbabilityDescription')"/>
												<Button icon="sap-icon://delete" text="Delete all" press=".onDeleteAll.call($controller,$event, $controller.getView().getBindingContext().getPath() + '/Class')"/>
												<ToolbarSpacer/>
												<Text text="Уровень" fieldGroupIds="gLvl"/>
												<Input id="lvlFrom" value="{ path:'Lvl/From' }" liveChange="handleLiveChangeLvl" width="10%" visible="true" type="Number" fieldGroupIds="gLvl" fieldWidth="10%"/>
												<Input id="lvlTo"   value="{ path:'Lvl/To'   }" liveChange="handleLiveChangeLvl" width="10%" visible="true" type="Number" fieldGroupIds="gLvl" fieldWidth="10%"  />
											</Toolbar>
										</headerToolbar>
										<columns>
											<Column>
												<Text text="Name"/>
											</Column>
											<Column>
												<Text text="Description"/>
											</Column>
											<Column>
												<Text text="Probabilty"/>
											</Column>
											<Column/>
										</columns>
										<items>
											<ColumnListItem type="Active">
												<cells>
													<Input value="{Name}"/>
												</cells>
												<cells>
													<Input value="{Description}"/>
												</cells>
												<cells>
													<Input value="{Probability}"/>
												</cells>
												<cells>
													<Button icon="sap-icon://delete" press=".onDelete.call($controller,$event, $controller.getView().getBindingContext().getPath() + '/Class')"/>
												</cells>
											</ColumnListItem>
										</items>
									</Table>
								</ScrollContainer>
							</l:BlockLayoutCell>
						</l:BlockLayoutRow>
					</l:BlockLayout>
				</IconTabFilter>
			</items>
		</IconTabBar>
		<footer>
			<Bar>
				<contentLeft>
					<Button icon="sap-icon://nav-back" press="onNavBack"/>
				</contentLeft>
			</Bar>
		</footer>
	</Page>
</mvc:View>