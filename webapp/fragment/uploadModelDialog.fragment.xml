<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc" xmlns:u="sap.ui.unified"
	class="viewPadding">
	<Dialog id="uploadModelDialog" title="Загрузка">
		<l:VerticalLayout>
			<u:FileUploader id="fileUpload" name="myFileUpload"  tooltip="Загрузка" uploadComplete="onHandleUploadComplete"
			change="onHandleValueChange"
			typeMissmatch="onHandleTypeMissmatch"
			style="Emphasized"
			fileType="json"
			
			/>
			<Button text="Загрузка" press="onHandleUploadPress"/>
		</l:VerticalLayout>
	</Dialog>
</core:FragmentDefinition>